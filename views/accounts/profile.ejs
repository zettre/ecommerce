<% layout('layout') -%>

<div class="container">
<div class="row">
<div class="col-sm-6 col-md-4">
<img style="width:200px; height:200px;" class="img-circle img-responsive img-center" src="<%= user.profile.picture %>" alt="">
<br>
<p>Name: <%= user.profile.name %></p>
<p>Email: <%= user.email %></p>
<p>Address: 
<% if(user.address==="" || typeof user.address==='undefined')  { } else{ %>
<%= user.address%>
<% } %>
</p>
<a href="/edit-profile" class="btn btn-primary">Edit Profile</a>
</div>
</div>
</div>